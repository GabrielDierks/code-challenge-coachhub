  "dependencies": {
    "axios": "0.18.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "node-sass-chokidar": "1.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
